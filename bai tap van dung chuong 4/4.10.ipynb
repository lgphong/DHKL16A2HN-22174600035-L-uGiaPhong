    "import numpy as np\n",
    "import pandas as pd \n",
    "\n",
    "ser = pd.Series(np.random.randint(1,10,7))\n",
    "\n",
    "df = pd.DataFrame(ser, columns = ['Numbers'])\n",
    "\n",
    "boi_3 = df[df['Numbers'] % 3 == 0].index \n",
    "\n",
    "print(\"Vị trí của các số có bội 3 trong ser là :\\n\",boi_3)\n"
   ]
  }
